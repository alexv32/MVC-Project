

@{
    ViewBag.Title = "MemberPage";
}

<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/jquery.validate.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.js"></script>

<h2>MemberPage</h2>

<table class="table table-bordered table-responsive table-hover" id="tblMember">
    <tr>
        <th>Joined</th> <td id="Joined"></td>
    </tr>
    <tr>
        <th>Number Of Points</th> <td id="Points"></td>
    </tr>
    <tr>
        <th>Expiration</th> <td id="Expiration"></td>
    </tr>
</table>

<script>
    $.get("MemberData", null, createTable)
    function createTable(data) {
        $("#Joined").text(data[0].Joined);
        $("#Points").text(data[0].Points);
        $("#Expiration").text(data[0].Expiration);
    }
</script>